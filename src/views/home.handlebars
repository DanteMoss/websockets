<h1 class="mainTitle">Productos agregados hasta el momento</h1>
<div class="generalContainer">
    {{#each products}}
    <div class="containerProductWithId">
        <p class="idProducto">{{this.id}}</p>
        <div class="containerProductInfo">
            <h1>{{this.title}}</h1>
            <p><b>Descripción:</b> {{this.description}}</p>
            <p><b>Código:</b> {{this.code}}</p>
            <p><b>Stock:</b> {{this.stock}}</p>
            <p><b>Precio:</b> {{this.price}}</p>
            <p><b>Categoria:</b> {{this.category}}</p>
        </div>
    </div>
    {{/each}}
</div>